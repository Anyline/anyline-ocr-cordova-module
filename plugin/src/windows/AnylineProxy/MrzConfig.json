{
        "captureResolution":"1080p",

        "cutout": {
                "style": "rect",
                "maxWidthPercent": "90%",
                "maxHeightPercent": "90%",
                "alignment": "center",
                "strokeWidth": 2,
                "cornerRadius": 4,
                "strokeColor": "FFFFFF",
                "outerColor": "000000",
                "outerAlpha": 0.3,
                "feedbackStrokeColor": "0099FF",
                "cropOffset": {
                        "x": 10,
                        "y": 20
                }
        },
        "flash": {
                "mode": "manual",
                "alignment": "bottom_right"
        },
        "beepOnResult": true,
        "vibrateOnResult": true,
        "blinkAnimationOnResult": true,
        "cancelOnResult": true,
        "visualFeedback": {
                "style": "RECT",
                "strokeColor": "0099FF",
                "fillColor": "220099FF",
                "animationDuration": 70
        }
}